{
  "name": "aev-thewarden",
  "version": "3.0.0",
  "description": "AEV (Autonomous Extracted Value) - TheWarden: Intelligent autonomous agent competing with MEV through learning-based value extraction",
  "main": "dist/src/main.js",
  "author": "StableExo",
  "license": "MIT",
  "scripts": {
    "start": "node dist/src/main.js",
    "start:mainnet": "./scripts/launch-mainnet.sh",
    "start:autonomous": "./scripts/autonomous-run.sh",
    "preload:pools": "npm run build && node dist/scripts/preload-pools.js",
    "preload:pools:force": "npm run build && node dist/scripts/preload-pools.js --force",
    "status": "./scripts/status.sh",
    "dev": "ts-node src/main.ts > logs/arbitrage.log 2>&1",
    "dev:watch": "ts-node src/main.ts",
    "dev:kill": "kill $(lsof -t -i:3000)",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:reasoning": "jest tests/unit/reasoning",
    "test:memory": "jest tests/unit/memory",
    "test:cognitive": "jest tests/unit/cognitive",
    "test:ethics": "jest tests/unit/ethics",
    "test:mev": "jest tests/unit/mev",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:anvil": "forge test --fork-url ${ARBITRUM_RPC_URL} --fork-block-number 228000000 -vvv",
    "test:anvil:gas": "forge test --fork-url ${ARBITRUM_RPC_URL} --gas-report",
    "test:pools": "ts-node scripts/test-pool-detection.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "validate-env": "ts-node scripts/validate-env.ts",
    "validate-mainnet": "node scripts/validate-mainnet-config.js",
    "verify-key": "node scripts/verify-private-key.js",
    "mainnet": "npm run validate-mainnet && npm run build && npm start",
    "check": "npm run lint && npm run format:check && npm run build && npm test",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy-executor-v3.ts",
    "deploy:testnet": "hardhat run scripts/deploy-executor-v3.ts --network goerli",
    "deploy:mainnet": "hardhat run scripts/deploy-executor-v3.ts --network mainnet",
    "deploy:flashswapv2": "hardhat run scripts/deployFlashSwapV2.ts --network base",
    "deploy:flashswapv2:testnet": "hardhat run scripts/deployFlashSwapV2.ts --network baseSepolia",
    "audit:slither": "slither . --fail-medium",
    "audit:slither:report": "slither . --print human-summary",
    "audit:prod": "npm audit --omit=dev --audit-level=high"
  },
  "dependencies": {
    "@aave/core-v3": "1.19.3",
    "@solana/web3.js": "1.98.4",
    "@types/uuid": "8.3.4",
    "@uniswap/sdk-core": "4.0.7",
    "@uniswap/v3-core": "1.0.1",
    "@uniswap/v3-periphery": "1.4.3",
    "@uniswap/v3-sdk": "3.8.3",
    "alchemy-sdk": "3.6.5",
    "amqplib": "0.10.3",
    "async-mutex": "0.5.0",
    "bcrypt": "5.1.1",
    "cors": "2.8.5",
    "csv-parser": "3.2.0",
    "dotenv": "16.3.1",
    "ethers": "5.7.2",
    "express": "4.21.2",
    "geoip-lite": "1.4.10",
    "ioredis": "5.3.2",
    "ip-address": "10.0.1",
    "jsonwebtoken": "9.0.2",
    "ms": "2.1.3",
    "node-vault": "0.10.9",
    "p-queue": "6.6.2",
    "pg": "8.11.3",
    "pino": "9.14.0",
    "pino-pretty": "13.0.0",
    "qrcode": "1.5.4",
    "socket.io": "4.8.1",
    "speakeasy": "2.0.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "2.2.3",
    "@nomiclabs/hardhat-etherscan": "3.1.7",
    "@openzeppelin/contracts": "4.9.6",
    "@types/amqplib": "0.10.1",
    "@types/bcrypt": "5.0.2",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/geoip-lite": "1.4.4",
    "@types/jest": "29.5.3",
    "@types/jsonwebtoken": "9.0.10",
    "@types/ms": "2.1.0",
    "@types/node": "20.4.5",
    "@types/node-vault": "0.9.13",
    "@types/qrcode": "1.5.6",
    "@types/speakeasy": "2.0.10",
    "@typescript-eslint/eslint-plugin": "8.46.4",
    "@typescript-eslint/parser": "8.46.4",
    "chai": "4.3.7",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "hardhat": "2.22.5",
    "hardhat-gas-reporter": "1.0.9",
    "hardhat-tracer": "3.2.1",
    "jest": "29.6.2",
    "prettier": "3.0.0",
    "prettier-plugin-solidity": "1.1.3",
    "solidity-coverage": "0.8.16",
    "ts-jest": "29.4.5",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=20.18.0",
    "npm": ">=10.2.4"
  },
  "overrides": {
    "elliptic": "6.6.1",
    "flat": "5.0.1",
    "minimatch": "3.0.5",
    "body-parser": "1.20.3",
    "ws": "8.17.1",
    "braces": "3.0.3",
    "js-yaml": "4.1.1"
  }
}
