{
  "name": "aev-thewarden",
  "version": "5.1.0",
  "description": "AEV (Autonomous Extracted Value) - TheWarden: Intelligent autonomous agent competing with MEV through learning-based value extraction",
  "type": "module",
  "main": "src/main.ts",
  "author": "StableExo",
  "license": "MIT",
  "scripts": {
    "start": "node --import tsx src/main.ts",
    "start:mainnet": "./scripts/launch-mainnet.sh",
    "start:autonomous": "./TheWarden",
    "start:monitor": "./TheWarden --monitor",
    "monitor": "./TheWarden --monitor",
    "monitor:consciousness": "node --import tsx scripts/consciousness-monitor.ts",
    "autonomous:control": "node --import tsx scripts/autonomous-warden-controller.ts",
    "warden:collab": "node --import tsx scripts/live-collaboration-interface.ts",
    "warden:collab:auto": "node --import tsx scripts/live-collaboration-interface.ts --auto-start",
    "add:dex": "node --import tsx scripts/add-dex.ts",
    "preload:pools": "node --import tsx scripts/preload-pools.ts",
    "preload:pools:force": "node --import tsx scripts/preload-pools.ts --force",
    "status": "./scripts/status.sh",
    "dev": "node --import tsx src/main.ts",
    "dev:watch": "node --import tsx --watch src/main.ts",
    "dev:kill": "kill $(lsof -t -i:3000)",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "build:clean": "rm -rf dist && npm run build",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:reasoning": "vitest run tests/unit/reasoning",
    "test:memory": "vitest run tests/unit/memory",
    "test:cognitive": "vitest run tests/unit/cognitive",
    "test:ethics": "vitest run tests/unit/ethics",
    "test:mev": "vitest run tests/unit/mev",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage",
    "test:anvil": "forge test --fork-url ${ARBITRUM_RPC_URL} --fork-block-number 228000000 -vvv",
    "test:anvil:gas": "forge test --fork-url ${ARBITRUM_RPC_URL} --gas-report",
    "test:pools": "node --import tsx scripts/test-pool-detection.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:all": "eslint .",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "format:all": "prettier --write \"**/*.{ts,js,json,md}\" --ignore-path .gitignore",
    "validate-env": "node --import tsx scripts/validate-env.ts",
    "validate-mainnet": "node --import tsx scripts/validate-mainnet-config.ts",
    "verify-key": "node --import tsx scripts/verify-private-key.ts",
    "verify:aerodrome": "node --import tsx scripts/verify-aerodrome.ts",
    "compare-env": "node --import tsx scripts/compare-env-files.ts",
    "mainnet": "npm run validate-mainnet && npm run start",
    "check": "npm run lint && npm run format:check && npm run typecheck && npm run test",
    "check:all": "npm run lint:all && npm run format:check && npm run typecheck && npm run test:ci",
    "clean": "rm -rf dist coverage .eslintcache",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy-executor-v3.ts",
    "deploy:testnet": "hardhat run scripts/deploy-executor-v3.ts --network goerli",
    "deploy:mainnet": "hardhat run scripts/deploy-executor-v3.ts --network mainnet",
    "deploy:flashswapv2": "hardhat run scripts/deployFlashSwapV2.ts --network base",
    "deploy:flashswapv2:testnet": "hardhat run scripts/deployFlashSwapV2.ts --network baseSepolia",
    "deploy:flashswapv2:verified": "VERIFY_CONTRACT=true hardhat run scripts/deployFlashSwapV2.ts --network base",
    "verify:flashswapv2": "hardhat run scripts/verifyFlashSwapV2.ts --network base",
    "verify:flashswapv2:testnet": "hardhat run scripts/verifyFlashSwapV2.ts --network baseSepolia",
    "contract:status": "node --import tsx scripts/etherscan-api.ts status",
    "contract:abi": "node --import tsx scripts/etherscan-api.ts getabi",
    "audit:slither": "slither . --fail-medium",
    "audit:slither:report": "slither . --print human-summary",
    "audit:deps": "npm outdated || true",
    "promote-memory": "npm run build && node --import tsx scripts/promote-memory.cjs",
    "run:20cycles": "./scripts/run-20-cycles.sh",
    "autonomous:learn": "node --import tsx scripts/autonomous-cycle-runner.ts",
    "cycle": "node --import tsx scripts/run-single-cycle.ts",
    "cycle:next": "node --import tsx scripts/run-single-cycle.ts"
  },
  "dependencies": {
    "@aave/core-v3": "1.19.3",
    "@solana/web3.js": "1.98.4",
    "alchemy-sdk": "3.6.5",
    "amqplib": "0.10.9",
    "argon2": "0.44.0",
    "async-mutex": "0.5.0",
    "better-sqlite3": "12.5.0",
    "cors": "2.8.5",
    "csv-parser": "3.2.0",
    "dotenv": "17.2.3",
    "ethers": "6.15.0",
    "express": "5.1.0",
    "ioredis": "5.8.2",
    "ip-address": "10.1.0",
    "jsonwebtoken": "9.0.2",
    "ms": "2.1.3",
    "node-vault": "0.10.9",
    "p-queue": "9.0.1",
    "pg": "8.16.3",
    "pino": "10.1.0",
    "pino-pretty": "13.1.2",
    "qrcode": "1.5.4",
    "socket.io": "4.8.1",
    "speakeasy": "2.0.0",
    "uuid": "13.0.0",
    "viem": "2.40.3",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.1",
    "@nomicfoundation/hardhat-ethers": "4.0.3",
    "@nomicfoundation/hardhat-verify": "3.0.7",
    "@nomicfoundation/hardhat-viem": "3.0.1",
    "@openzeppelin/contracts": "5.4.0",
    "@types/amqplib": "0.10.8",
    "@types/better-sqlite3": "7.6.13",
    "@types/cors": "2.8.19",
    "@types/dotenv": "6.1.1",
    "@types/express": "5.0.5",
    "@types/jsonwebtoken": "9.0.10",
    "@types/ms": "2.1.0",
    "@types/node": "24.10.1",
    "@types/qrcode": "1.5.6",
    "@types/speakeasy": "2.0.10",
    "@typescript-eslint/eslint-plugin": "8.48.0",
    "@typescript-eslint/parser": "8.48.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "globals": "16.5.0",
    "hardhat": "3.0.16",
    "prettier": "3.7.3",
    "prettier-plugin-solidity": "2.2.0",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vitest": "4.0.14"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "overrides": {
    "elliptic": "6.6.1",
    "flat": "5.0.1",
    "minimatch": "3.0.5",
    "body-parser": "1.20.3",
    "ws": "8.17.1",
    "braces": "3.0.3",
    "js-yaml": "4.1.1",
    "cookie": "0.7.2",
    "tmp": "0.2.5",
    "hardhat": {
      "@nomicfoundation/hardhat-zod-utils": {
        "zod": "3.25.76"
      }
    },
    "@nomicfoundation/hardhat-verify": {
      "@nomicfoundation/hardhat-zod-utils": {
        "zod": "3.25.76"
      }
    }
  }
}
