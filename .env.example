# ğŸ”’ PRIVATE ORDER-FLOW / MEV-FRIENDLY RPCs
# â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€

# Enable private transaction submission (keeps txs out of public mempool)
ENABLE_PRIVATE_RPC=true

# Privacy level: none | basic | enhanced | maximum
# - none: Public mempool (no privacy)
# - basic: Flashbots Protect (https://docs.flashbots.net/flashbots-protect/overview)
# - enhanced: MEV-Share with hints (https://docs.flashbots.net/flashbots-mev-share/overview)
# - maximum: Builder direct with no hints
PRIVATE_RPC_PRIVACY_LEVEL=basic

# Flashbots Protect RPC (Mainnet)
FLASHBOTS_RPC_URL=https://rpc.flashbots.net
FLASHBOTS_AUTH_KEY=                 # Optional: Your Flashbots auth key

# MEV-Share Endpoint
MEV_SHARE_RPC_URL=https://relay.flashbots.net
MEV_SHARE_AUTH_KEY=                 # Optional: MEV-Share auth key

# Builder-specific private RPC endpoints (optional)
BUILDER_RPC_URL_1=                  # Example: Titan, Rsync, etc.
BUILDER_RPC_AUTH_KEY_1=

# Private RPC Configuration
PRIVATE_RPC_TIMEOUT=30000           # Timeout for private submission (ms)
PRIVATE_RPC_FALLBACK=true           # Fallback to public mempool if private fails
PRIVATE_RPC_FAST_MODE=false         # Submit to multiple relays simultaneously

# MEV-Share Hints (what to share about your transaction)
MEV_SHARE_HINT_CALLDATA=false       # Share transaction calldata
MEV_SHARE_HINT_CONTRACT=false       # Share contract address
MEV_SHARE_HINT_FUNCTION=false       # Share function selector
MEV_SHARE_HINT_LOGS=false           # Share logs

