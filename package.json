{
  "name": "copilot-consciousness",
  "version": "3.0.0",
  "description": "Advanced arbitrage bot with PROJECT-HAVOC integration",
  "main": "dist/main.js",
  "author": "StableExo",
  "license": "MIT",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "ts-node src/main.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy-executor-v3.ts",
    "deploy:testnet": "hardhat run scripts/deploy-executor-v3.ts --network goerli",
    "deploy:mainnet": "hardhat run scripts/deploy-executor-v3.ts --network mainnet"
  },
  "dependencies": {
    "@aave/core-v3": "^1.19.3",
    "@openzeppelin/contracts": "^4.9.3",
    "@solana/web3.js": "^1.98.4",
    "@uniswap/sdk-core": "^4.0.7",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.3",
    "@uniswap/v3-sdk": "^3.11.2",
    "amqplib": "^0.10.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "socket.io": "^4.6.2"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@types/amqplib": "^0.10.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "chai": "^4.3.7",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "hardhat": "^2.19.5",
    "hardhat-gas-reporter": "^1.0.9",
    "jest": "^29.6.2",
    "prettier": "^3.0.0",
    "prettier-plugin-solidity": "^1.1.3",
    "solidity-coverage": "^0.8.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
