# Controls whether TheWarden uses preloaded (cached) pool data or live network data.
# 
# ğŸ”´ LIVE DATA IS NOW THE DEFAULT! 
# The system is optimized enough to fetch pool data in real-time.
# This ensures accurate profit calculations and real trading opportunities.
#
# Data Mode Priority:
#   1. Default (no flags)         â†’ LIVE DATA from network (production mode)
#   2. USE_PRELOADED_POOLS=true   â†’ Use cached data (development/testing only)
#   3. OFFLINE_CACHE_ONLY=true    â†’ Use stale cached data even when expired

# Set to 'true' ONLY for development/testing to use cached pool data
# Default: false (live data is fetched directly from network)
# USE_PRELOADED_POOLS=true

# Pool data cache duration in minutes (only applies when USE_PRELOADED_POOLS=true)
# Set to 0 for no caching
POOL_CACHE_DURATION=60

# Offline cache only mode - use this when network is unavailable
# When true, forces use of preloaded data even if stale
# OFFLINE_CACHE_ONLY=true

