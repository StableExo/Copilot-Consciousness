{
  "name": "aev-thewarden",
  "version": "5.1.0",
  "description": "AEV (Autonomous Extracted Value) - TheWarden: Intelligent autonomous agent competing with MEV through learning-based value extraction",
  "type": "module",
  "main": "src/main.ts",
  "author": "StableExo",
  "license": "MIT",
  "scripts": {
    "start": "bun src/main.ts",
    "start:mainnet": "./scripts/launch-mainnet.sh",
    "start:autonomous": "./TheWarden",
    "start:monitor": "./TheWarden --monitor",
    "monitor": "./TheWarden --monitor",
    "add:dex": "bun scripts/add-dex.ts",
    "preload:pools": "bun scripts/preload-pools.ts",
    "preload:pools:force": "bun scripts/preload-pools.ts --force",
    "status": "./scripts/status.sh",
    "dev": "bun src/main.ts",
    "dev:watch": "bun --watch src/main.ts",
    "dev:kill": "kill $(lsof -t -i:3000)",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "build:clean": "rm -rf dist && bun run build",
    "test": "bun test",
    "test:unit": "bun test tests/unit",
    "test:integration": "bun test tests/integration",
    "test:e2e": "bun test tests/e2e",
    "test:reasoning": "bun test tests/unit/reasoning",
    "test:memory": "bun test tests/unit/memory",
    "test:cognitive": "bun test tests/unit/cognitive",
    "test:ethics": "bun test tests/unit/ethics",
    "test:mev": "bun test tests/unit/mev",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch",
    "test:ci": "bun test --coverage",
    "test:vitest": "vitest run",
    "test:anvil": "forge test --fork-url ${ARBITRUM_RPC_URL} --fork-block-number 228000000 -vvv",
    "test:anvil:gas": "forge test --fork-url ${ARBITRUM_RPC_URL} --gas-report",
    "test:pools": "bun scripts/test-pool-detection.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:all": "eslint .",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "format:all": "prettier --write \"**/*.{ts,js,json,md}\" --ignore-path .gitignore",
    "validate-env": "bun scripts/validate-env.ts",
    "validate-mainnet": "bun scripts/validate-mainnet-config.ts",
    "verify-key": "bun scripts/verify-private-key.ts",
    "verify:aerodrome": "bun scripts/verify-aerodrome.ts",
    "compare-env": "bun scripts/compare-env-files.ts",
    "mainnet": "bun run validate-mainnet && bun run start",
    "check": "bun run lint && bun run format:check && bun run typecheck && bun run test",
    "check:all": "bun run lint:all && bun run format:check && bun run typecheck && bun run test:ci",
    "clean": "rm -rf dist coverage .eslintcache",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy-executor-v3.ts",
    "deploy:testnet": "hardhat run scripts/deploy-executor-v3.ts --network goerli",
    "deploy:mainnet": "hardhat run scripts/deploy-executor-v3.ts --network mainnet",
    "deploy:flashswapv2": "hardhat run scripts/deployFlashSwapV2.ts --network base",
    "deploy:flashswapv2:testnet": "hardhat run scripts/deployFlashSwapV2.ts --network baseSepolia",
    "deploy:flashswapv2:verified": "VERIFY_CONTRACT=true hardhat run scripts/deployFlashSwapV2.ts --network base",
    "verify:flashswapv2": "hardhat run scripts/verifyFlashSwapV2.ts --network base",
    "verify:flashswapv2:testnet": "hardhat run scripts/verifyFlashSwapV2.ts --network baseSepolia",
    "contract:status": "bun scripts/etherscan-api.ts status",
    "contract:abi": "bun scripts/etherscan-api.ts getabi",
    "audit:slither": "slither . --fail-medium",
    "audit:slither:report": "slither . --print human-summary",
    "audit:prod": "bun pm trust --all && bun audit",
    "audit:deps": "bun outdated || true",
    "promote-memory": "bun run build && bun scripts/promote-memory.cjs"
  },
  "dependencies": {
    "@aave/core-v3": "1.19.3",
    "@solana/web3.js": "1.98.4",
    "@uniswap/sdk-core": "7.9.0",
    "@uniswap/v3-core": "1.0.1",
    "@uniswap/v3-periphery": "1.4.4",
    "@uniswap/v3-sdk": "3.26.0",
    "alchemy-sdk": "3.6.5",
    "amqplib": "0.10.9",
    "async-mutex": "0.5.0",
    "cors": "2.8.5",
    "csv-parser": "3.2.0",
    "ethers": "6.15.0",
    "express": "5.1.0",
    "ioredis": "5.8.2",
    "ip-address": "10.1.0",
    "jsonwebtoken": "9.0.2",
    "ms": "2.1.3",
    "node-vault": "0.10.9",
    "p-queue": "9.0.1",
    "pg": "8.16.3",
    "pino": "10.1.0",
    "pino-pretty": "13.1.2",
    "qrcode": "1.5.4",
    "socket.io": "4.8.1",
    "speakeasy": "2.0.0",
    "uuid": "13.0.0",
    "viem": "2.40.3",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.1",
    "@nomicfoundation/hardhat-verify": "2.1.3",
    "@nomicfoundation/hardhat-viem": "3.0.1",
    "@openzeppelin/contracts": "4.9.6",
    "@types/amqplib": "0.10.8",
    "@types/bun": "latest",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.5",
    "@types/jsonwebtoken": "9.0.10",
    "@types/ms": "2.1.0",
    "@types/node": "24.10.1",
    "@types/qrcode": "1.5.6",
    "@types/speakeasy": "2.0.10",
    "@typescript-eslint/eslint-plugin": "8.48.0",
    "@typescript-eslint/parser": "8.48.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "globals": "16.5.0",
    "hardhat": "2.27.0",
    "prettier": "3.7.2",
    "prettier-plugin-solidity": "2.2.0",
    "typescript": "5.9.3",
    "vitest": "4.0.14"
  },
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=22.12.0"
  },
  "overrides": {
    "elliptic": "6.6.1",
    "flat": "5.0.1",
    "minimatch": "3.0.5",
    "body-parser": "1.20.3",
    "ws": "8.17.1",
    "braces": "3.0.3",
    "js-yaml": "4.1.1",
    "cookie": "0.7.2",
    "tmp": "0.2.5",
    "@uniswap/v3-staker": "1.0.2"
  }
}
